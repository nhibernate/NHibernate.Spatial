			<type name="NHibernate.Spatial.Type.GeometryType,NHibernate.Spatial">
				<param name="subtype">MULTIPOLYGON</param>
				<param name="srid">4326</param>
			</type>
		</property>
	</class>
</hibernate-mapping>
